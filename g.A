bits 64
%macro M 1-*
  %rep (%0)
    %1
    %rotate  1
  %endrep
%endmacro
%macro C 1-*
  mov   rax, rdi
  mov   edi, 'C'
  mov   rsi, 'C'
  mov   rdx, rsi
  lea   rcx, [rel %%next]
  jmp   rax
  %rep (%0)
    %1
    %rotate  1
  %endrep
  %%next:
%endmacro
%macro G 1
  %defstr %%name %1
  mov   rax, rdi
  mov   edi, 'G'
  mov   rsi, %%name
  lea   rdx, [rel %1]
  lea   rcx, [rel %%next]
  jmp   rax
  %%next:
%endmacro
%macro A 1
  %defstr %%name %1
  mov   rax, rdi
  mov   edi, 'A'
  mov   rsi, %%name
  lea   rdx, [rel %1]
  lea   rcx, [rel %%next]
  jmp   rax
  %%next:
%endmacro
%macro T 1
  mov   rax, rdi
  mov   edi, 'T'
  mov   rsi, %1
  mov   rdx, rsi
  lea   rcx, [rel %%next]
  jmp   rax
  %%next:
%endmacro

_:
 ret

enalfaaf:
  M {C T "A", G _}, {C T "a", G _}
  M {C T "B", G _}, {C T "b", G _}
  M {C T "C", G _}, {C T "c", G _}
  M {C T "D", G _}, {C T "d", G _}
  M {C T "E", G _}, {C T "e", G _}
  M {C T "F", G _}, {C T "f", G _}
  dq 0
enalfagz:                  
  M {C T "G", G _}, {C T "g", G _}
  M {C T "H", G _}, {C T "h", G _}
  M {C T "I", G _}, {C T "i", G _}
  M {C T "J", G _}, {C T "j", G _}
