  M {C T "L", G _}, {C T "l", G _}
  M {C T "M", G _}, {C T "m", G _}
  M {C T "N", G _}, {C T "n", G _}
  M {C T "O", G _}, {C T "o", G _}
  M {C T "P", G _}, {C T "p", G _}
  M {C T "Q", G _}, {C T "q", G _}
  M {C T "R", G _}, {C T "r", G _}
  M {C T "S", G _}, {C T "s", G _}
  M {C T "T", G _}, {C T "t", G _}
  M {C T "U", G _}, {C T "u", G _}
  M {C T "V", G _}, {C T "v", G _}
  M {C T "W", G _}, {C T "w", G _}
  M {C T "X", G _}, {C T "x", G _}
  M {C T "Y", G _}, {C T "y", G _}
  M {C T "Z", G _}, {C T "z", G _}

